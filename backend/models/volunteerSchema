import mongoose from 'mongoose';

const volunteerSchema=new moongose.Schema({
    name:{
        type:String,
        required: [true,"Name is Required"]
    },
    password:{
        type:String,
        required: [true,"Password is Required"]
    },
    doubtSessionPending:{
        type:Boolean
    },
    course:{
        type:String,
        required: [true,"course is Required"]
    }
});

const volunteer = mongoose.model('lessonNotes', volunteerSchema);

export default volunteer;
